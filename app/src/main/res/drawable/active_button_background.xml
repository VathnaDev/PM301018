<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#0E1C65" />
            <corners android:radius="12dp" />
            <padding android:bottom="16dp" android:left="16dp" android:right="16dp" android:top="16dp" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#0E1C65" />
            <corners android:radius="12dp" />
            <padding android:left="16dp" android:right="16dp" android:top="16dp" />
            <stroke android:color="#992BB5FF" android:width="4dp" android:dashGap="16dp"/>
        </shape>
    </item>

</selector>